<section class="home-content py-20">
    <section class="ml-20 certificate_container py-20">

            <h3 class="sub-heading"> Account Details </h3>
            <h1 class="heading"> Check Your Account </h1>
            <% if(university.role === "University") { %>
            <form>
                <div class="row">
                    <h4 class="mb-10">My Account</h4>
                    <div class="input-group input-group-icon">
                        <h5 class="mt-2 ml-20"><%= university.uniName %></h5>
                        <span class="input-icon">
                            <i class="fa fa-user"></i>
                        </span>
                    </div>
                    <div class="input-group input-group-icon">
                        <h5 class="mt-2 ml-20"><%= university.officialemail %></h5>
                        <span class="input-icon">
                            <i class="fa fa-envelope"></i>
                        </span>
                    </div>
                    <div class="input-group input-group-icon">
                        <h5 class="mt-2 ml-20"><%= university.phoneOne %></h5>
                        <span class="input-icon">
                            <i class="fas fa-mobile-alt"></i>
                        </span>
                    </div>
                    <div class="input-group input-group-icon">
                        <h5 class="mt-2 ml-20"><%= university.phonetwo %></h5>
                        <span class="input-icon">
                            <i class="fas fa-mobile-alt"></i>
                        </span>
                    </div>
                    <div class="input-group input-group-icon">
                        <h5 class="mt-2 ml-20"><%= university.phonethree %></h5>
                        <span class="input-icon">
                            <i class="fas fa-mobile-alt"></i>
                        </span>
                    </div>

                    <div class="row mt-4">
                        <div class="col-half">
                            <h5>District</h5>
                            <div class="input-group">
                                    <h5><%= university.district %></h5>
                            </div>
                        </div>
                        <div class="col-half">
                            <h5>City</h5>
                            <div class="input-group">
                                <h5><%= university.city %></h5>
                            </div>
                        </div>
                    </div>

                    <div class="row mt-4">
                        <div class="col-half">
                            <h5>ZipCode</h5>
                            <div class="input-group">
                                    <h5><%= university.zipcode %></h5>
                            </div>
                        </div>
                        <div class="col-half">
                            <h5>First Creator Name</h5>
                            <div class="input-group">
                                    <h5><%= university.nameOne %></h5>
                            </div>
                        </div> 
                    </div>

                    <div class="row mt-4">
                        <div class="col-half">
                            <h5>First Creator Email</h5>
                                <div class="input-group">
                                        <h5><%= university.emailOne %></h5>
                            </div>
                        </div>
                        <div class="col-half">
                            <h5>First Creator Designation</h5>
                            <div class="h5-group">
                                    <h5><%= university.designationOne %></h5>
                            </div>
                        </div> 
                    </div>
                    <div class="row mt-4">
                        <div class="col-half">
                            <h5>Second Creator Name</h5>
                            <div class="input-group">
                                    <h5><%= university.nameTwo %></h5>
                            </div>
                        </div> 
                        <div class="col-half">
                            <h5>Second Creator Email</h5>
                                <div class="input-group">
                                   <h5><%= university.emailTwo %></h5>
                            </div>
                        </div>
                    </div>

                    <div class="row mt-4">
                        <div class="col-half">
                            <h5>Second Creator Designation</h5>
                            <div class="h5-group">
                                    <h5><%= university.designationTwo %></h5>
                            </div>
                        </div> 
                        <div class="col-half">
                            <h5>Granted Date</h5>
                            <div class="input-group">
                                <div class="col-third">
                                    <h5><%= moment(university.updatedAt).format('DD:MM:YYYY') %></h5>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="btn"></div>
                <a class="float-right mt-6 text-2xl" href="/university/dashboard">Go Back</a>
            </form>
            <% } %> 
        </section>
</section>